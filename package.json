{
  "name": "azure-servicebus-explorer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "paths": {
    "backend": "backend",
    "frontend": "frontend"
  },
  "scripts": {
    "test": "mocha --exit --timeout 15000 --recursive */*.spec.js",
    "start": "npm run backend & npm run frontend",
    "install-backend": "cd $npm_package_paths_backend && npm i",
    "install-frontend": "cd $npm_package_paths_frontend && npm i",
    "install-app": "npm run install-backend && npm run install-frontend",
    "backend": "cd $npm_package_paths_backend && forever server.js",
    "frontend": "cd $npm_package_paths_frontend && npm start"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.2.0",
    "forever": "^2.0.0",
    "mocha": "^7.1.1",
    "sinon": "^9.0.2",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@azure/service-bus": "^1.1.6",
    "@koa/cors": "^3.0.0",
    "dotenv": "^8.2.0",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^8.0.8"
  }
}
